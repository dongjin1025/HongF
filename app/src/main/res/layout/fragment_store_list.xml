<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context="com.dongjin.android.hongf.view.StoreListFragment"
    android:background="@drawable/listbackground">

    <!-- TODO: Update blank fragment layout -->
    <!--<include-->
        <!--android:id="@+id/tb_maps"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content"-->
        <!--layout="@layout/toolbar_main"-->
        <!--android:visibility="visible"-->
        <!--android:layout_alignParentEnd="true" />-->
    <include
        android:id="@+id/tb_maps"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        layout="@layout/toolbar_main"
        android:visibility="visible"
        android:layout_alignParentEnd="true" />


    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:layout_marginTop="25dp">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp">

                <ImageView
                    android:layout_height="100dp"
                    app:srcCompat="@drawable/korean"
                    android:id="@+id/list_ig_korean"
                    android:layout_width="100dp" />

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp">

                <ImageView
                    android:layout_height="100dp"
                    app:srcCompat="@drawable/japan"
                    android:id="@+id/list_ig_japan"
                    android:layout_width="100dp" />

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp">

                <ImageView
                    app:srcCompat="@drawable/china"
                    android:id="@+id/list_ig_china"
                    android:layout_height="100dp"
                    android:layout_width="100dp" />

            </LinearLayout>

        </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp">

            <ImageView
                android:layout_height="100dp"
                app:srcCompat="@drawable/western"
                android:id="@+id/list_ig_western"
                android:layout_width="100dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_margin="5dp">

            <ImageView
                android:layout_height="100dp"
                app:srcCompat="@drawable/koreansnack"
                android:id="@+id/list_ig_koreanSnack"
                android:layout_width="100dp" />

            <TextView
                android:text="분식"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/tv_koreansnack"
                android:layout_weight="1"
                android:gravity="center_horizontal"
                android:textColor="@color/black" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_margin="5dp">

            <ImageView
                app:srcCompat="@drawable/world"
                android:id="@+id/list_ig_world"
                android:layout_height="100dp"
                android:layout_width="100dp" />

            <TextView
                android:text="기타 세계 음식"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/tv_world"
                android:layout_weight="1"
                android:gravity="center_horizontal"
                android:textColor="@color/black" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp">

            <ImageView
                android:layout_height="100dp"
                app:srcCompat="@drawable/fastfood"
                android:id="@+id/list_ig_fastfood"
                android:layout_width="100dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_margin="5dp">

            <ImageView
                android:layout_height="100dp"
                app:srcCompat="@drawable/pub"
                android:id="@+id/list_ig_bar"
                android:layout_width="100dp" />

            <!--<TextView-->
                <!--android:text="술집"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:id="@+id/tv_bar"-->
                <!--android:layout_weight="1"-->
                <!--android:gravity="center_horizontal"-->
                <!--android:textColor="@color/black" />-->
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_margin="5dp">

            <ImageView
                app:srcCompat="@drawable/cafe"
                android:id="@+id/list_ig_cafe"
                android:layout_height="100dp"
                android:layout_width="100dp" />

            <!--<TextView-->
                <!--android:text="디저트/까페"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:id="@+id/tv_cafe"-->
                <!--android:layout_weight="1"-->
                <!--android:gravity="center_horizontal"-->
                <!--android:textColor="@color/black" />-->
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="125dp"
        android:layout_marginLeft="16dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_margin="5dp">

            <ImageView
                app:srcCompat="@drawable/all"
                android:id="@+id/list_ig_all"
                android:layout_height="100dp"
                android:layout_width="100dp" />

            <!--<TextView-->
                <!--android:text="All"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:id="@+id/tv_all"-->
                <!--android:layout_weight="1"-->
                <!--android:gravity="center_horizontal"-->
                <!--android:textColor="@color/black"/>-->

        </LinearLayout>

        <ImageView
            app:srcCompat="@drawable/logoooo"
            android:id="@+id/imageView8"
            android:layout_weight="0.15"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="28dp"
            android:layout_height="90dp"
            android:layout_width="90dp" />


    </LinearLayout>


</LinearLayout>

